
<article id="age">

  <div class="container">
    <h2>How old is the patient?</h2>

    <article id="button2">

      <select onchange={{action "setSelection" value="target.value"}} id="age_cat">
        <option disabled selected>Age?</option>
        <option value="infant">0-1</option>
        <option value="young children">2-9</option>
        <option value="adults">10-64</option>
        <option value="elderly">65+</option>
      </select>
      {{#link-to "initial-symptoms"}}<button {{action "submit"}} {{action "setup"}} id="select_button">Submit</button>{{/link-to}}

    </article>

  </div>

</article>